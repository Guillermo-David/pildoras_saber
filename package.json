{
  "name": "pildoras",
  "version": "0.0.0",
  "main": "electron-main.ts",
  "type": "commonjs",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run start-server\"",
    "build-server": "tsc --project ./",
    "start-server": "npm run build-server && node ./dist/server/server.js",
    "start-server-0": "ts-node ./server/server.ts",
    "start-server-2": "node --loader ts-node/esm ./server/server.ts",
    "start-server-3": "ts-node --esm ./server/server.ts",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron .",
    "electron-build": "ng build && electron .",
    "winpack": "electron-packager ./ pildoras_saber --platform-win32 --overwrite",
    "init-db": "ts-node ./server/init-db.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.2",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.1.2",
    "@angular/compiler": "^17.1.2",
    "@angular/core": "^17.1.2",
    "@angular/forms": "^17.1.2",
    "@angular/material": "^17.1.2",
    "@angular/platform-browser": "^17.1.2",
    "@angular/platform-browser-dynamic": "^17.1.2",
    "@angular/router": "^17.1.2",
    "@prisma/client": "^5.9.1",
    "body-parser": "^1.20.2",
    "clipboard": "^2.0.11",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "font-awesome": "^4.7.0",
    "marked": "^9.0.0",
    "ngx-markdown": "^17.1.1",
    "prismjs": "^1.28.0",
    "rxjs": "~7.8.0",
    "sqlite3": "^5.1.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "^17.1.2",
    "@angular/compiler-cli": "^17.1.2",
    "@types/jasmine": "~4.3.0",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prisma": "^5.9.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  }
}
